{
  "name": "apitest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "set DEBUG=app:*,express:* & node-dev ./bin/www",
    "test": "mocha --timeout 10000",
    "testd": "set DEBUG=app:*,test:*,sequelize:sql:* & mocha --timeout 10000",
    "lint": "eslint .",
    "migrate-dev": "sequelize db:migrate --env development",
    "migrate-test": "sequelize db:migrate --env test"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "moment-timezone": "^0.5.25",
    "morgan": "~1.9.1",
    "sequelize": "^5.8.6",
    "sqlite3": "^4.0.8",
    "uuid-token-generator": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^9.1.0",
    "mocha": "^6.1.4",
    "sequelize-cli": "^5.4.0"
  }
}
